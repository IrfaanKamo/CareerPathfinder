                <td class="text-center">@Html.RadioButtonFor(x => x.q1_1, "false", new { @class = "test-element" })</td>
            </tr>
            <tr>
                <td></td>
                <td></td>
                <td>I think that some jobs give more status than others</td>
                <td class="text-center">@Html.RadioButtonFor(x => x.q1_2, "true", new { @class = "test-element" })</td>
                <td class="text-center">@Html.RadioButtonFor(x => x.q1_2, "false", new { @class = "test-element" })</td>
            </tr>
            <tr>
                <td></td>
                <td></td>
